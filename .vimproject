   layout=layout{
    top_menu.haml
    top_menu=top_menu{
     favourites.haml
     finder.haml
     user.haml
    }
   } // layout

   job_result=job_result{
    annotations.haml
    array.haml
    binary.haml
    boolea.haml
    boolean.haml
    integer.haml
    job_control.haml
    string.haml
    text.haml
    tsv.haml
    yaml.haml
   } // job_result

   help=help{
    entity.haml
    workflow.haml
   } // help
  }
 }
}
