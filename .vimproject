     _mixins.sass
     _vars.sass
     base.sass
     layout.sass
     skeleton.sass
    }

    foundation.sass
    solarized.sass

    theme.sass
    colors.sass
    variables.sass

    functions.sass
    style.sass

    entity_card.sass
    actions.sass
    favourites.sass
    finder.sass
    user.sass
    workflow.sass
    help.sass

    figure.sass

    top_menu.sass
    rbbt=rbbt{
     form.sass
     fragment.sass
     menu.sass
     reveal.sass
     table.sass
    }
    _rbbt.sass
    layout.sass
    app.sass
   } // compass

   js=public/js{
    deffer.js
    jquery-ui.js
    _ajax_replace.js
    _md5.js
    _ellipsis.js

    helpers.js

    rbbt=rbbt{
     actions.js
     dom_update.js
     favourites.js
     fragment.js
     hide.js
     list.js
     map.js
     menu.js
     modal.js
     page_type.js
     reveal.js
     table.js
     workflow.js
    }

    app.js
   } // js

   entity=entity{
    Default.haml
   } // entity

   entity_list=entity_list{
    Default.haml
    Default=Default{
     edit.haml
     new.haml
    }
   } // entity_list

   entity_map=entity_map{
    Default.haml
   } // entity_map

   partials=partials{
    form.haml
    table.haml
    table=table{
     column.haml
     files.haml
     filters.haml
     page.haml
    }
   } // partials

   entity_partials=entity_partials{
    action_card.haml
    action_controller.haml
    entity_card.haml
    entity_list.haml
    entity_list_card.haml
    entity_map_card.haml
    list_container.haml
   } // entity_partials
