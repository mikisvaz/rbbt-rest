     list_container.rb
     action_controller.rb
    }
    entity.rb

    workflow=workflow{
     locate.rb
     jobs.rb
     render.rb
    }
    workflow.rb

    common=common{
     users.rb

     locate.rb
     resources.rb

     misc.rb
     forms.rb
     table.rb

     render.rb
     cache.rb
    }
    main.rb
   }
  }
 }
 etc=etc{
  workflows
 }
