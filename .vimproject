rbbt-rest=/$PWD filter="*.config *.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 config.ru
 workflow.rb
 Rakefile
 lib=lib {
  rbbt=rbbt{
   rest=rest{
    client.rb

    helpers.rb

    file_server.rb

    web_tool.rb

    knowledge_base.rb
    knowledge_base=knowledge_base{
     helpers.rb
    }

    entity=entity{
     rest.rb

     helpers.rb
