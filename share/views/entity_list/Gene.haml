.entity_list_card
  .title
    %span.list_type= list.entity_type
    %span.list_id= CGI.unescape(list_id)

  .entities
    %h3 Entities
    %ul.entities
      - list.each do |gene|
        %li= gene.link

- action_controller = ActionController.new(list, list_id)
- action_controller.add :enrichment, "Enrichment Analysis"

.section
  = action_controller_render action_controller

-#.section
  .actions
    - actions =  %w(enrichment)
    %ul.actions
      - actions.each do |action|
        %li
          = list.list_action_link action, action, list_id
      %li.reload_action
        %a(href='#') Reload
    .action
