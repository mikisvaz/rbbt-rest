= action_card_render ActionCard.new do
  = table do
    - changes = list.collect do |l| 
      - begin
        - context = l.bases_in_range(-1..1)
      - rescue
        - next
      - [context, l.base] * ">"
    - counts = Misc.counts(changes.compact) 

    - TSV.setup(counts, :key_field => "Change", :fields => ["Count"], :type => :single, :cast => :to_i)
    - counts
